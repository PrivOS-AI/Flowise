{
    "name": "@flowise/image-gen-mcp-server",
    "version": "2.0.0",
    "description": "Unified MCP Server for Image Generation: Google Gemini/Imagen or Self-Hosted ComfyUI",
    "type": "module",
    "main": "dist/image-gen-server.js",
    "bin": {
        "image-gen-server": "./dist/image-gen-server.js",
        "flux-gen-server": "./dist/flux-gen-server.js"
    },
    "scripts": {
        "build": "tsc -p tsconfig.json",
        "clean": "rm -rf dist",
        "rebuild": "npm run clean && npm run build",
        "start": "node dist/image-gen-server.js",
        "start:comfyui": "node dist/flux-gen-server.js"
    },
    "dependencies": {
        "@google/genai": "^1.25.0",
        "@modelcontextprotocol/sdk": "^1.10.1",
        "axios": "^1.6.0",
        "sharp": "^0.33.5"
    },
    "devDependencies": {
        "typescript": "^5.0.0",
        "@types/node": "^20.0.0"
    }
}
